<% @params[:entries].each do |entry| -%>
<% if entry[:all_day] -%>
<% if entry[:start_date] == entry[:end_date] -%>
<%= entry[:start_date].strftime('%Y-%m-%d') %>〜<%= entry[:end_date].strftime('%Y-%m-%d') %>
<% else -%>
<%= entry[:start_date].strftime('%Y-%m-%d') %>
<% end -%>
<% else -%>
<%= entry[:start_date].strftime('%Y-%m-%d %H:%M') %>〜<%= entry[:end_date].strftime('%Y-%m-%d %H:%M') %>
<% end -%>

<% if @params[:tag] -%><%= entry[:title] %><% end %>
<% if @params[:body] -%><%= entry[:body] %><% end %>
<% if @params[:location] -%><%= entry[:location] %><% end %>
<% end -%>

<% if @params[:tag] && params[:source] -%>
<%= @params[:source].create_tags(@params[:status]).join(' ') %>
<% end -%>
